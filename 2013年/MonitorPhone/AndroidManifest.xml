<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
	package="com.frank.linux" android:versionCode="1" android:sharedUserId="android.uid.phone"
	android:versionName="1.0">
	<uses-sdk android:minSdkVersion="9" />
	<uses-permission android:name="android.permission.READ_PHONE_STATE"></uses-permission>
	<uses-permission android:name="android.permission.MODIFY_PHONE_STATE"></uses-permission>
	<uses-permission android:name="android.permission.CALL_PHONE"></uses-permission>
	<application android:icon="@drawable/happy_robot" android:process="com.android.phone"
		android:label="@string/app_name">
		<activity android:name=".MonitorPhoneActivity" android:label="@string/app_name">
			<intent-filter>
				<action android:name="android.intent.action.MAIN" />
				<category android:name="android.intent.category.LAUNCHER" />
			</intent-filter>
		</activity>

	</application>
</manifest>