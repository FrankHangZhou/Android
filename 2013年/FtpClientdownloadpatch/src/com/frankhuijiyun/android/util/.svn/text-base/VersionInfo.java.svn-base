package com.frankhuijiyun.android.util;

/**
 * 软件版本信息对象 根据服务器上的说明文档 2011-11-05 如下格式
 * <kernelversion>2.0_20120530</kernelversion>
 * <androidversion>2.0_20120530</androidversion>
 * <updateTime>2012-05-30</updateTime>
 * <downloadkernelURL>http://192.168.250.115/77777777/</downloadURL>
 * <downloadandroidURL>http://192.168.250.115/77777777/</downloadURL>
 * <displayMessage>新版本发布,亲 ## 1.新增A功能 ## 2.新增B功能## 3.新增C功能</displayMessage>
 * 
 * 
 */
public class VersionInfo {

	// 内核版本描述字符串
	private String kernelversion;
	// android版本
	private String androidversion;
	// 版本更新时间
	private String updateTime;
	// 内核新版本更新下载地址
	private String downloadkernelURL;
	// android新版本更新下载地址
	private String downloadandroidURL;

	// 更新描述信息
	private String displayMessage;
	// 版本号
	private int versionCode;

	public String getkernelVersion() {
		return kernelversion;
	}

	public void setkernelVersion(String kernelversion) {
		this.kernelversion = kernelversion;
	}

	public String getAndroidVersion() {
		return androidversion;
	}

	public void setAndroidVersion(String androidversion) {
		this.androidversion = androidversion;
	}

	public String getUpdateTime() {
		return updateTime;
	}

	public void setUpdateTime(String updateTime) {
		this.updateTime = updateTime;
	}

	public String getDownloadKernelURL() {
		return downloadkernelURL;
	}

	public void setDownloadKernelURL(String downloadkernelURL) {
		this.downloadkernelURL = downloadkernelURL;
	}

	public String getDownloadAndroidURL() {
		return downloadandroidURL;
	}

	public void setDownloadAndroidURL(String downloadandroidURL) {
		this.downloadandroidURL = downloadandroidURL;
	}

	public String getDisplayMessage() {
		return displayMessage;
	}

	public void setDisplayMessage(String displayMessage) {
		this.displayMessage = displayMessage;
	}

	public int getVersionCode() {
		return versionCode;
	}

	public void setVersionCode(int versionCode) {
		this.versionCode = versionCode;
	}

}
